@model IEnumerable<TimetableSystem.Models.Request>
@{
    ViewBag.Title = "Requests";
}
<h3>Your Requests</h3>

<p>
    Select from @Model.Count() genres:</p>
<ul>
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
    }
</ul>

