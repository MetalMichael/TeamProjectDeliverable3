@model IEnumerable<TimetableSystem.Models.Request>
@{
    ViewBag.Title = "Requests";
}
<h3>Your Requests</h3>

<p>
    Select from @Model.Count() requests:</p>
<ul>
    @foreach (var request in Model)
    {
        <li>@Html.ActionLink(request.RequestId.ToString(), "Edit", "Home", new { id = request.RequestId }, new { target="_self" })</li>
    }
</ul>